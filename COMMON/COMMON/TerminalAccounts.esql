BROKER SCHEMA COMMON


CREATE PROCEDURE getTerminalAccount(IN TERMINALID CHARACTER , OUT ACCOUNT CHARACTER ) BEGIN 

	SET ACCOUNT = THE(SELECT ITEM T.ACCOUNTNO FROM Database.ZBDB_ESB.DB2INST1.TERMINAL AS T WHERE T.TERMINALID = TERMINALID);


END;	

CREATE PROCEDURE getTerminalUSDAccount(IN TERMINALID CHARACTER , OUT ACCOUNT CHARACTER ) BEGIN 


	SET ACCOUNT = THE(SELECT ITEM T.USDACCOUNTNO FROM Database.ZBDB_ESB.DB2INST1.TERMINAL AS T WHERE T.TERMINALID = TERMINALID);
	
END;