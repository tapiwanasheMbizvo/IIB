

CREATE COMPUTE MODULE LOG_AUTOPAY
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		-- CALL CopyEntireMessage();
		
	
		
		RETURN TRUE;
	END;



CREATE PROCEDURE logAUTOPAY() BEGIN 

INSERT INTO Database.ZBDB_ESB.DB2INST1.AUTOPAYMENT(
CBSACCOUNT
)

VALUES (InputRoot.DFDL.AUTOPAY.CBSACCOUNT);

END;
	
	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
