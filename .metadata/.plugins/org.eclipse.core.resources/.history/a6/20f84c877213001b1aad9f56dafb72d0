       	SET responseCode = getResponseCode(XRREC,MsgTxt ); 
       		IF responseCode IS NULL THEN 
       		SET responseCode ='124';
       	END IF;                          
       	SET OutputRoot.DFDL.ns:ISO8583_1993.ActionCode_039                             = responseCode;
       	
       						
	
        SET OutputRoot.DFDL.ns:ISO8583_1993.CardAcceptorTerminalIdentification_041     = ISOMSG.CardAcceptorTerminalIdentification_041;
        SET OutputRoot.DFDL.ns:ISO8583_1993.CardAcceptorNameLocation_043               = ISOMSG.CardAcceptorNameLocation_043;
        SET OutputRoot.DFDL.ns:ISO8583_1993.AdditionalDataPrivate_048                  = ISOMSG.AdditionalDataPrivate_048;
        SET OutputRoot.DFDL.ns:ISO8583_1993.CurrencyCodeTransaction_049                = ISOMSG.CurrencyCodeTransaction_049;
        SET OutputRoot.DFDL.ns:ISO8583_1993.CurrencyCodeReconciliation_050             = ISOMSG.CurrencyCodeReconciliation_050;
        SET OutputRoot.DFDL.ns:ISO8583_1993.CurrencyCodeCardholderBilling_051          = ISOMSG.CurrencyCodeCardholderBilling_051;
      
        
        IF OutputRoot.DFDL.ns:ISO8583_1993.PrimaryBitmap.Bit046 = '1'THEN 
        	SET OutputRoot.DFDL.ns:ISO8583_1993.AmountsFees_046 =ISOMSG.AmountsFees_046;
        ELSE
        	--SET OutputRoot.DFDL.ns:ISO8583_1993.PrimaryBitmap.Bit046 = 0;
        END IF;
       
        IF OutputRoot.DFDL.ns:ISO8583_1993.SecondaryBitmap.Bit102 = '1'THEN 
