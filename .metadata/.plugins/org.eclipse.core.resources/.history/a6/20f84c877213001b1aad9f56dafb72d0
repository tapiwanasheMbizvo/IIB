        END IF;	
        
        
        IF OutputRoot.DFDL.ns:ISO8583_1993.SecondaryBitmap.Bit103 = '1'THEN 
        	SET OutputRoot.DFDL.ns:ISO8583_1993.AccountIdentification2_103                 = ISOMSG.AccountIdentification2_103;
        	
        END IF;	
        CALL addToMQDestinationList(InputLocalEnvironment ,'ISO_LOGGER_IN');
		CALL addToMQDestinationList(InputLocalEnvironment ,'TO_IP');

		
			RETURN TRUE;			
			ELSE	--END TXN RSP IS NOT NULL
			--the txn resp is null 
			--raise alarm EQUATION has not responded to the TXN REQUEST 
			--CALL timeOutException();
			--save the bad ref
			DECLARE EXPIRED_REF CHARACTER;
			
			SET EXPIRED_REF = ISOMSG.RetrievalReferenceNumber_037;
			
			--save this ref and wait for responses from equation 
			
			
		ELSEIF ISOMSG.RetrievalReferenceNumber_037 IS NULL  THEN --THE MESSAGES EXPIRED BECAUSE THEY COULD NOT FIND AN ISO REQUEST , CAUSED BY A TIMEOUT , THESE ARE MESSAGES THAT DELAYED ABOVE
			IF TRANSACTION_RESP.TRANSACTION_RSP_HEADER.MsgTxt IS NOT NULL THEN 
			END IF;	
		
		END IF;		
END IF ;
		 				
	END;
	

	
	
		
	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	
	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;

