CREATE FUNCTION getISZBTERMINAL(IN TERMINALID CHARACTER ) RETURNS BOOLEAN BEGIN
 IF TERMINAL_CACHE.TERMINAL.TERMINALID IS NULL THEN
 -- load the cache

 
CALL loadCACHE();

 END IF;	
	RETURN THE(SELECT ITEM T.TERMINALID FROM TERMINAL_CACHE.TERMINAL[] AS T
 	 WHERE T.TERMINALID = TERMINALID) IS NOT NULL;
END;